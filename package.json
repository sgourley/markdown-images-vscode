{
  "name": "markdown-images",
  "displayName": "Markdown Images",
  "description": "Show images in markdown preview from a photo library",
  "publisher": "sgourley",
  "author": "Steven Gourley",
  "license": "MIT",
  "version": "0.2.0",
  "icon": "ui/logo.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "markdown",
    "images"
  ],
  "main": "./extension.js",
  "browser": "./extension.js",
  "engines": {
    "vscode": "^1.34.0"
  },
  "activationEvents": [],
  "contributes": {
    "markdown.markdownItPlugins": true,
    "configuration": {
      "title": "Markdown Images",
      "properties": {
        "markdown.images.library.url": {
          "type": "string",
          "description": "URL of image library to use when previewing markdown for image references beginning with '!'.",
          "default": ""
        }
      }
    }
  },
  "homepage": "https://github.com/sgourley/markdown-images-vscode#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgourley/markdown-images-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/sgourley/markdown-images-vscode/issues"
  },
  "devDependencies": {
    "vsce": "^2.8.0"
  },
	"scripts": {
		"package": "vsce package"
	}
}
